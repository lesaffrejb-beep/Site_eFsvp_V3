{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "efsvp/cta",
  "title": "Call-to-Action",
  "category": "efsvp-blocks",
  "icon": "megaphone",
  "description": "Section appel à l'action premium avec fond coloré ou image",
  "keywords": ["cta", "call-to-action", "button", "conversion", "efsvp"],
  "version": "1.0.0",
  "textdomain": "efsvp",
  "supports": {
    "align": ["wide", "full"],
    "anchor": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Prêt à donner vie à votre histoire ?"
    },
    "description": {
      "type": "string",
      "default": "Partagez-nous votre projet et découvrons ensemble comment le magnifier."
    },
    "buttonText": {
      "type": "string",
      "default": "Démarrer votre projet"
    },
    "buttonUrl": {
      "type": "string",
      "default": "#contact"
    },
    "secondaryButtonText": {
      "type": "string",
      "default": ""
    },
    "secondaryButtonUrl": {
      "type": "string",
      "default": ""
    },
    "backgroundImage": {
      "type": "object",
      "default": null
    },
    "backgroundColor": {
      "type": "string",
      "default": "primary"
    },
    "overlayOpacity": {
      "type": "number",
      "default": 0.8
    }
  },
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}
