{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "efsvp/portfolio",
  "title": "Portfolio Grid",
  "category": "efsvp-blocks",
  "icon": "format-gallery",
  "description": "Galerie de projets portfolio avec catégories",
  "keywords": ["portfolio", "gallery", "projects", "efsvp"],
  "version": "1.0.0",
  "textdomain": "efsvp",
  "supports": {
    "align": ["wide", "full"],
    "anchor": true
  },
  "attributes": {
    "sectionTitle": {
      "type": "string",
      "default": "Portfolio"
    },
    "sectionSubtitle": {
      "type": "string",
      "default": "Nos projets récents"
    },
    "projects": {
      "type": "array",
      "default": []
    },
    "postsPerPage": {
      "type": "number",
      "default": -1
    },
    "columns": {
      "type": "number",
      "default": 3
    }
  },
  "editorStyle": "file:./editor.css",
  "style": "file:./style.css"
}
